{
  "name": "@lodasoft/api-ts",
  "version": "1.0.0",
  "description": "Autogenerated typescript client library for lodasoft",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build-all": "npm run get-spec && npm run generate && tsc",
    "generate": "npx @nll/api-codegen-ts",
    "get-spec": "wget http://devapi.lodasoft.com/swagger/docs/v1 -O spec.json",
    "prepublishOnly": "npm run test && npm run build",
    "release": "npx standard-version",
    "test": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baetheus/lodasoft-api-ts.git"
  },
  "keywords": [
    "lodasoft",
    "typescript"
  ],
  "author": "Brandon Blaylock (brandon@null.pub)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/baetheus/lodasoft-api-ts/issues"
  },
  "homepage": "https://github.com/baetheus/lodasoft-api-ts#readme",
  "devDependencies": {
    "@nll/api-codegen-ts": "^0.1.3",
    "standard-version": "~6.0.1",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "fp-ts": "^2.0.3",
    "io-ts": "^2.0.0",
    "rxjs": "^6.5.2"
  },
  "apiCodegen": {
    "src": "./spec.json",
    "dst": "./src"
  }
}
